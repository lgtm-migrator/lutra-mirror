<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <Match>
    <!-- Template inherits equals and hashCode from KnowledgeBase -->
    <Class name="xyz.ottr.lutra.store.TemplateNode" />
    <Bug pattern="EQ_DOESNT_OVERRIDE_EQUALS" />
  </Match>
  <Match>
    <!-- Generated sources -->
    <Source name="~.*stOTTR.*"/>
  </Match>
  <Match>
    <!-- try with resources with Java 11 triggers RCN warnings https://github.com/spotbugs/spotbugs/issues/756 -->
    <Class name="xyz.ottr.lutra.tabottr.parser.ExcelReader" />
    <Bug pattern="RCN_REDUNDANT_NULLCHECK_WOULD_HAVE_BEEN_A_NPE" />
  </Match>

  <Match>
    <!-- This is an overloaded method, which is in fact called. -->
    <Class name="xyz.ottr.lutra.bottr.model.ValueMap" />
    <Method name="getRDFNode" />
    <Bug pattern="UPM_UNCALLED_PRIVATE_METHOD" />
  </Match>

</FindBugsFilter>
