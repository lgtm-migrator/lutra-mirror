@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

@prefix ottr:   <http://ns.ottr.xyz/templates#> .

@prefix ex:   <http://example.com#> .


ex:Test3Template a ottr:Template ;
  ottr:hasParameter 
     [ ottr:index 1; ottr:variable ex:y1 ] ,
     [ ottr:index 2; ottr:variable ex:y2 ] ,
     [ ottr:index 3; ottr:variable ex:y3 ] .

[] ottr:templateRef ex:Triple ;
   ottr:withValues ( ex:y1 rdf:type ottr:none ) .

[] ottr:templateRef ex:TestTemplate ;
   ottr:withValues ( ex:y1 ex:y2 ottr:none ) .

[] ottr:templateRef ex:Triple ;
   ottr:withValues ( ex:y3 rdf:type ex:Type3 ) .
